<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dexd"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="313dp"
        android:layout_height="39dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="35dp"
        android:gravity="center"
        android:text="Contact"
        android:textColor="#ffffff"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="45dp"
        android:background="@drawable/white_background"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="158dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="150dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="Entreprise NTG"
            android:textColor="#000000"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="NotSibling" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="82dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="220dp"
            android:gravity="center"
            android:text="Consulting"
            android:textColor="#A5A5A5"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ScrollView
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginTop="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView11">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintlayout5"
                        android:layout_width="320dp"
                        android:layout_height="65dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="20dp"
                        android:background="@drawable/white_edittext"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/constraintlayout3"
                        tools:ignore="DuplicateIds,MissingConstraints">

                        <com.google.android.material.floatingactionbutton.FloatingActionButton
                            android:id="@+id/FABDirector"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:layout_marginStart="28dp"
                            android:backgroundTint="@color/white"
                            android:clickable="true"
                            android:foregroundGravity="center"
                            android:src="@drawable/baseline_person_24"
                            app:backgroundTint="@color/white"
                            app:borderWidth="0dp"
                            app:fabCustomSize="48dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.47"
                            app:tint="@color/black" />

                        <TextView
                            android:id="@+id/titledirector"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="24dp"
                            android:text="Notre Directeur"
                            android:textColor="@color/black"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toEndOf="@+id/FABDirector"
                            app:layout_constraintTop_toTopOf="@+id/FABDirector" />

                        <TextView
                            android:id="@+id/director"
                            android:layout_width="181dp"
                            android:layout_height="20dp"
                            android:layout_marginStart="24dp"
                            android:layout_marginTop="5dp"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="@+id/FABDirector"
                            app:layout_constraintStart_toEndOf="@+id/FABDirector"
                            app:layout_constraintTop_toBottomOf="@+id/titledirector" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintlayout4"
                        android:layout_width="320dp"
                        android:layout_height="65dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="20dp"
                        android:background="@drawable/white_edittext"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.475"
                        app:layout_constraintStart_toEndOf="@+id/constraintlayout3"
                        app:layout_constraintTop_toBottomOf="@+id/constraintlayout1"
                        tools:ignore="DuplicateIds,MissingConstraints">

                        <com.google.android.material.floatingactionbutton.FloatingActionButton
                            android:id="@+id/FABmap"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:layout_marginStart="28dp"
                            android:backgroundTint="@color/white"
                            android:clickable="true"
                            android:foregroundGravity="center"
                            android:src="@drawable/baseline_map_24"
                            app:backgroundTint="@color/white"
                            app:borderWidth="0dp"
                            app:fabCustomSize="48dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.47"
                            app:tint="@color/design_default_color_error" />

                        <TextView
                            android:id="@+id/titlemap"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="24dp"
                            android:text="Notre Adresse"
                            android:textColor="@color/black"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toEndOf="@+id/FABmap"
                            app:layout_constraintTop_toTopOf="@+id/FABmap" />

                        <TextView
                            android:id="@+id/map"
                            android:layout_width="200dp"
                            android:layout_height="30dp"
                            android:layout_marginStart="24dp"
                            android:layout_marginTop="10dp"
                            android:text="04 rue emir abdelkhrim, sidi bel abbes, algerie"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="@+id/FABmap"
                            app:layout_constraintStart_toEndOf="@+id/FABmap"
                            app:layout_constraintTop_toBottomOf="@+id/titlemap"
                            app:layout_constraintVertical_bias="0.714" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintlayout3"
                        android:layout_width="320dp"
                        android:layout_height="65dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/white_edittext"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
                        tools:ignore="DuplicateIds">

                        <com.google.android.material.floatingactionbutton.FloatingActionButton
                            android:id="@+id/FABweb"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:layout_marginStart="28dp"
                            android:backgroundTint="@color/white"
                            android:clickable="true"
                            android:foregroundGravity="center"
                            android:src="@drawable/baseline_web_24"
                            app:backgroundTint="@color/white"
                            app:borderWidth="0dp"
                            app:fabCustomSize="48dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.47"
                            app:tint="@color/blue" />

                        <TextView
                            android:id="@+id/titleweb"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="24dp"
                            android:layout_marginTop="5dp"
                            android:text="Site Web"
                            android:textColor="@color/black"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toEndOf="@+id/FABweb"
                            app:layout_constraintTop_toTopOf="@+id/FABweb" />

                        <TextView
                            android:id="@+id/web"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="24dp"
                            android:text="www.enie.dz"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="@+id/FABweb"
                            app:layout_constraintStart_toEndOf="@+id/FABweb"
                            app:layout_constraintTop_toBottomOf="@+id/titleweb" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintlayout1"
                        android:layout_width="320dp"
                        android:layout_height="65dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="20dp"
                        android:background="@drawable/white_edittext"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.488"
                        app:layout_constraintStart_toEndOf="@+id/constraintLayout2"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="DuplicateIds">

                        <com.google.android.material.floatingactionbutton.FloatingActionButton
                            android:id="@+id/FABmail"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:layout_marginStart="28dp"
                            android:backgroundTint="@color/white"
                            android:clickable="true"
                            android:foregroundGravity="center"
                            android:src="@drawable/baseline_mail"
                            app:backgroundTint="@color/white"
                            app:borderWidth="0dp"
                            app:fabCustomSize="48dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.47"
                            app:tint="@color/orange" />

                        <TextView
                            android:id="@+id/titlemail"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="24dp"
                            android:layout_marginTop="5dp"
                            android:text="Adresse E-mail"
                            android:textColor="@color/black"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toEndOf="@+id/FABmail"
                            app:layout_constraintTop_toTopOf="@+id/FABmail" />

                        <TextView
                            android:id="@+id/mail"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="24dp"
                            android:text="grabadalila@gmail.com"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="@+id/FABmail"
                            app:layout_constraintStart_toEndOf="@+id/FABmail"
                            app:layout_constraintTop_toBottomOf="@+id/titlemail"
                            tools:ignore="DuplicateIds" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayout2"
                        android:layout_width="320dp"
                        android:layout_height="65dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/white_edittext"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <com.google.android.material.floatingactionbutton.FloatingActionButton
                            android:id="@+id/FABCall"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:layout_marginStart="28dp"
                            android:backgroundTint="@color/white"
                            android:clickable="true"
                            android:foregroundGravity="center"
                            android:src="@drawable/baseline_call"
                            app:backgroundTint="@color/white"
                            app:borderWidth="0dp"
                            app:fabCustomSize="48dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.47"
                            app:tint="@color/green" />

                        <TextView
                            android:id="@+id/phonetitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="24dp"
                            android:layout_marginTop="5dp"
                            android:text="Numéro de téléphone"
                            android:textColor="@color/black"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            app:layout_constraintStart_toEndOf="@+id/FABCall"
                            app:layout_constraintTop_toTopOf="@+id/FABCall" />

                        <TextView
                            android:id="@+id/textView12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="24dp"
                            android:text="0696999818"
                            android:textColor="@color/black"
                            android:textSize="12sp"
                            app:layout_constraintBottom_toBottomOf="@+id/FABCall"
                            app:layout_constraintStart_toEndOf="@+id/FABCall"
                            app:layout_constraintTop_toBottomOf="@+id/phonetitle" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="110dp"
        android:layout_height="80dp"
        android:elevation="80dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout3"
        app:srcCompat="@drawable/company"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>